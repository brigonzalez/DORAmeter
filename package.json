  "homepage": "https://github.com/brigonzalez/dorameter#readme",
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-get-off-my-lawn": "^5.0.1",
    "jest": "^26.2.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
