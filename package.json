{
  "name": "dorameter",
  "version": "1.0.0",
  "description": "An app that tracks DORA metrics",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12.18.3",
    "yarn": ">=1.9.4"
  },
  "scripts": {
